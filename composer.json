{
    "name": "alexstewartja/laravel-attribute-observer",
    "description": "Observe (and react to) attribute changes made on Eloquent models.",
    "keywords": [
        "alexstewartja",
        "laravel",
        "laravel-attribute-observer"
    ],
    "homepage": "https://github.com/alexstewartja/laravel-attribute-observer",
    "license": "MIT",
    "authors": [
        {
            "name": "Alex Stewart",
            "email": "iamalexstewart@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.0",
        "spatie/laravel-package-tools": "^1.4.3",
        "illuminate/contracts": "^8.37"
    },
    "require-dev": {
        "nunomaduro/collision": "^5.3",
        "orchestra/testbench": "^6.15",
        "pestphp/pest": "^1.18",
        "pestphp/pest-plugin-laravel": "^1.1",
        "spatie/laravel-ray": "^1.23",
        "vimeo/psalm": "^4.8"
    },
    "autoload": {
        "psr-4": {
            "AlexStewartJA\\LaravelAttributeObserver\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "AlexStewartJA\\LaravelAttributeObserver\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "./vendor/bin/pest --no-coverage",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "AlexStewartJA\\LaravelAttributeObserver\\LaravelAttributeObserverServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
